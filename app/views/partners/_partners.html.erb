<div class="pmd-card pmd-z-depth">
  <div class="table-responsive">
    <table class="table table-hover pmd-table table-striped">
      <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Action</th>
      </tr>
      </thead>
      <tbody>
      <% partners.each do |partner| %>
        <tr>
          <td><%= partner.id %></td>
          <td><%= partner.name %></td>
          <td><%= partner.email %></td>
          <td>
            <%= link_to partner_path(partner), title: "View info",
              class: "btn btn-sm btn-primary pmd-btn-fab pmd-btn-raised pmd-ripple-effect" do %>
              <i class="fa fa-eye"></i>
            <% end %>
            <%= link_to edit_partner_path(partner), title: "Edit",
              class: "btn btn-sm btn-success pmd-btn-fab pmd-btn-raised pmd-ripple-effect" do %>
              <i class="fa fa-pencil"></i>
            <% end %>
            <%= link_to partner_path(partner), title: "Delete",
              class: "btn btn-sm btn-danger pmd-btn-fab pmd-btn-raised pmd-ripple-effect", method: :delete,
              data: { confirm: "Are you sure?" } do %>
              <i class="fa fa-trash"></i>
            <% end %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
<%= paginate partners, theme: "twitter-bootstrap-3" %>
